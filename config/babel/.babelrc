{
  "presets": [
    "es2015",
    "es2015-loose",
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    ["babel-plugin-react-intl", {
      "messagesDir": "./tmp/messages/",
      "enforceDescriptions": true
    }],
    "lodash",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-transform-react-constant-elements",
        ["babel-plugin-react-intl", {
          "messagesDir": "./tmp/messages/",
          "enforceDescriptions": true
        }]
      ]
    }
  }
}
